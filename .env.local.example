# Local Development Environment Configuration
# Copy this to .env.local and fill in your values

# Local Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_KEY=your_local_anon_key_here
NEXT_PUBLIC_SUPABASE_SERVICE_ROLE_KEY=your_local_service_role_key_here

# Local Database URL for Drizzle
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres

# Clerk Configuration (same for local and prod)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key
CLERK_SECRET_KEY=your_clerk_secret_key

# PostHog Configuration (optional for local dev)
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key
NEXT_PUBLIC_POSTHOG_HOST=https://your-posthog-host.com

# Environment
NODE_ENV=development
