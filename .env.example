# T3 Stack Environment Variables

# Clerk Authentication
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_KEY=your_supabase_anon_key

# Local Supabase Development (add these for local dev)
# NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
# NEXT_PUBLIC_SUPABASE_KEY=your_local_anon_key
# NEXT_PUBLIC_SUPABASE_SERVICE_ROLE_KEY=your_local_service_role_key

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# PostHog Analytics
NEXT_PUBLIC_POSTHOG_KEY=phc_your_posthog_key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Vercel AI Gateway Configuration
AI_GATEWAY_API_KEY=your_vercel_ai_gateway_api_key
AI_GATEWAY_MODEL=google/gemini-2.0-flash-lite
AI_GATEWAY_PROMPT="Tell me a joke"

# Fallback GOogle
GOOGLE_GENERATIVE_AI_API_KEY=your_google_generative_ai_api_key

# Environment
NODE_ENV=development
