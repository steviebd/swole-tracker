{
  "name": "swole-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun scripts/opennext-build.ts",
    "check": "bun run lint && bun run typecheck && bun run test",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push --force",
    "db:push:dev": "infisical run --env dev -- drizzle-kit push --force",
    "db:push:staging": "infisical run --env staging -- drizzle-kit push --force",
    "db:push:prod": "infisical run --env prod -- drizzle-kit push --force",
    "db:push:all": "bun run db:push:dev && bun run db:push:staging && bun run db:push:prod",
    "db:reset:staging": "infisical run --env staging -- drizzle-kit push --force",
    "db:reset:prod": "infisical run --env prod -- drizzle-kit push --force",
    "db:studio": "drizzle-kit studio",
    "deploy": "wrangler deploy --logpush",
    "dev": "NODE_ENV=development infisical run --env dev -- bun run dev:worker",
    "dev:next": "bunx next dev --port 8787",
    "dev:worker": "bun run update-wrangler-config && lsof -ti:8787 | xargs kill -9 2>/dev/null || true && wrangler dev --env dev",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 20",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix --max-warnings 20",
    "typecheck": "tsc --noEmit",
    "tokens:build": "bun scripts/generate-theme-tokens.ts && bun scripts/build-material3-css.ts",
    "update-wrangler-config": "bun scripts/update-wrangler-config.ts",
    "setup-d1-databases": "bun scripts/setup-d1-databases.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@opennextjs/cloudflare": "^1.11.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/query-sync-storage-persister": "^5.90.5",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.3",
    "@tanstack/react-query-persist-client": "^5.90.5",
    "@tanstack/react-store": "^0.8.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "@trpc/client": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@workos-inc/node": "^7.71.0",
    "ai": "5.0.68",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-browserify": "^3.12.1",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "framer-motion": "^12.23.24",
    "https-browserify": "^1.0.0",
    "lucide-react": "^0.545.0",
    "next": "^15.5.5",
    "oauth4webapi": "^3.8.2",
    "postgres": "^3.4.7",
    "posthog-js": "^1.275.3",
    "posthog-node": "^5.10.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.65.0",
    "recharts": "^3.2.1",
    "server-only": "^0.0.1",
    "stream-http": "^3.2.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@ai-sdk/provider": "2.0.0",
    "@ai-sdk/provider-utils": "3.0.12",
    "@cloudflare/workers-types": "^4.20251011.0",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.3.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/recharts": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.5",
    "eslint-plugin-drizzle": "^0.2.3",
    "jsdom": "^27.0.0",
    "msw": "^2.11.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "1.0.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  },
  "packageManager": "bun@1.2.21",
  "volta": {
    "node": "20.19.4"
  }
}
