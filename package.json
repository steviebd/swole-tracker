{
  "name": "swole-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production next build",
    "check": "bun run lint && bun run typecheck",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push --force",
    "db:push:dev": "infisical run --env dev -- drizzle-kit push --force",
    "db:push:staging": "infisical run --env staging -- drizzle-kit push --force",
    "db:push:prod": "infisical run --env prod -- drizzle-kit push --force",
    "db:push:all": "bun run db:push:dev && bun run db:push:staging && bun run db:push:prod",
    "db:reset:staging": "infisical run --env staging -- drizzle-kit push --force",
    "db:reset:prod": "infisical run --env prod -- drizzle-kit push --force",
    "db:studio": "drizzle-kit studio",
    "deploy": "wrangler deploy --logpush",
    "dev": "NODE_ENV=development infisical run -- bun run dev:worker",
    "dev:worker": "bun run update-wrangler-config && lsof -ti:8787 | xargs kill -9 2>/dev/null || true && wrangler dev --env dev",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 20",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix --max-warnings 20",
    "typecheck": "tsc --noEmit",
    "tokens:build": "bun scripts/generate-theme-tokens.ts && bun scripts/build-material3-css.ts",
    "update-wrangler-config": "bash scripts/update-wrangler-config.sh",
    "setup-d1-databases": "bash scripts/setup-d1-databases.sh",
    "test": "vitest",
    "test:ci": "vitest run --coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@opennextjs/cloudflare": "^1.9.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/query-sync-storage-persister": "^5.83.1",
    "@tanstack/react-query": "^5.84.2",
    "@tanstack/react-query-persist-client": "^5.84.2",
    "@trpc/client": "^11.4.4",
    "@trpc/react-query": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "@workos-inc/node": "^7.70.0",
    "ai": "5.0.30",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.6",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "next": "^15.5.2",
    "oauth4webapi": "^3.6.2",
    "postgres": "^3.4.7",
    "posthog-js": "^1.261.6",
    "posthog-node": "^5.6.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.2",
    "server-only": "^0.0.1",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@ai-sdk/google": "1.2.22",
    "@ai-sdk/openai": "1.3.24",
    "@ai-sdk/provider": "2.0.0",
    "@ai-sdk/provider-utils": "3.0.7",
    "@cloudflare/workers-types": "^4.20251003.0",
    "@emnapi/core": "1.5.0",
    "@emnapi/runtime": "1.5.0",
    "@emnapi/wasi-threads": "1.1.0",
    "@eslint/eslintrc": "^3.3.1",
    "@img/colour": "1.0.0",
    "@inquirer/ansi": "1.0.0",
    "@napi-rs/wasm-runtime": "0.2.12",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.2.21",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^24.3.0",
    "@types/phoenix": "1.6.6",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/recharts": "^2.0.1",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.4.6",
    "eslint-plugin-drizzle": "^0.2.3",
    "generator-function": "2.0.1",
    "happy-dom": "^19.0.2",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rettime": "0.7.0",
    "secure-json-parse": "2.7.0",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "1.0.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "until-async": "3.0.2",
    "vitest": "^3.2.4",
    "whatwg-url": "14.2.0",
    "wrap-ansi": "7.0.0"
  },
  "packageManager": "bun@1.2.21",
  "volta": {
    "node": "20.19.4"
  }
}
